{
  "testMode": true,

  "sourceDir": "T:/",
  "targetDir": "T:/",
  "fragmentDir": "T:/_Fragments/",
  "logDir": "./",
  "folderSearchDepth": 2,

  "thresholdDays": 7,
  "intervalMinutes": 1,

  "rules": [
    {
      "dest": "_ニュース",
      "match": "(ニュース[7７])|(ニュースウ[ォオ]ッチ[9９])"
    },
    {
      "dest": "コズミックフロント",
      "match": "コズミック　フロント"
    },
    {
      "dest": "ダーウィンが来た！",
      "match": "ダーウィンが来た"
    },
    {
      "dest": "プロフェッショナル仕事の流儀",
      "match": "プロフェッショナル　仕事の流儀"
    },
    {
      "dest": "中井精也の鉄道写真旅",
      "match": "中井精也の((てつたび！)|(鉄道写真旅))"
    },
    {
      "dest": "CYCLE AROUND JAPAN",
      "match": "CYCLE JAPAN"
    }
  ],

  "others": "_その他"
}